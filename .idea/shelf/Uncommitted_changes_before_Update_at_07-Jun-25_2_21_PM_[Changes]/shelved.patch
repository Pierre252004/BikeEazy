        <div class=\"col-md-6 col-lg-3\">\r\n            <div class=\"card text-center\">\r\n                <img src=\"img/Nahar%20Malik.jpg\" class=\"card-img-top rounded-circle mx-auto mt-3\" style=\"width: 120px; height: 120px; object-fit: cover;\" alt=\"Nahar Malik\">\r\n                <div class=\"card-body\">\r\n                    <h5 class=\"card-title\">Nahar Malik</h5>\r\n                    <p class=\"card-text\">Customer Experience Specialist</p>\r\n                    <a href=\"./AboutUs/NaharMalik\" class=\"btn btn-tech w-100\">Contact Me</a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"col-md-6 col-lg-3\">\r\n            <div class=\"card text-center\">\r\n                <img src=\"img/Tariq%20Benson.jpg\" class=\"card-img-top rounded-circle mx-auto mt-3\" style=\"width: 120px; height: 120px; object-fit: cover;\" alt=\"Tariq Benson\">\r\n                <div class=\"card-body\">\r\n                    <h5 class=\"card-title\">Tariq Benson</h5>\r\n                    <p class=\"card-text\">Sustainability & Partnerships Lead</p>\r\n                    <a href=\"./AboutUs/TariqBenson\" class=\"btn btn-tech w-100\">Contact Me</a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</section>\r\n\r\n<!-- Footer -->\r\n<footer class=\"text-center py-4\">\r\n    <p>Designed by Pierre Georges • © 2025 QuickCycle Repairs</p>\r\n    <a href=\"https://pierre252004.github.io/Portfolio/\">Pierre's portfolio</a>\r\n</footer>\r\n\r\n<script src=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"></script>\r\n</body>\r\n</html>\r\n
